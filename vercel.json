{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "functions": {
    "app/api/upload-campaign/route.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "env": {
    "STORACHA_SPACE_DID": "STORACHA_SPACE_DID"
  }
}